{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "index.py"
        }
    ],
  "functions": {
    "api/**": {
      "excludeFiles": "{.next,*.cache,node_modules,public,app, .git}/**"
    }
  }
}
